{
  "name": "@ui5-next/types",
  "version": "1.4.16",
  "private": false,
  "description": "openui5 types",
  "main": "index.js",
  "typings": "./bin/index.d.ts",
  "scripts": {
    "release": "standard-version --commit-all --no-verify && git push --follow-tags origin master",
    "build": "ts-node ./builder/builder && npm run test",
    "test": "jest --coverage && tsc --noEmit",
    "publish:tag": "npm run build && npm publish --access public --tag latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ui5-next/types.git"
  },
  "author": "Theo Sun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ui5-next/types/issues"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "builder/.*?__tests__/.*_test\\.ts$",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "builder/**/*.ts",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "testconfig.js",
      "package.json",
      "package-lock.json",
      "tsconfig.json"
    ],
    "transformIgnorePatterns": [
      "resources"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "homepage": "https://github.com/ui5-next/types#readme",
  "devDependencies": {
    "@types/handlebars": "^4",
    "@types/jest": "^25",
    "@types/lodash": "^4",
    "@types/node": "^11",
    "handlebars": "^4",
    "jest": "^25",
    "lodash": "^4",
    "node-fetch": "^2",
    "standard-version": "^7",
    "ts-jest": "^25",
    "ts-loader": "^4",
    "ts-node": "^8",
    "tslint": "^5",
    "tslint-loader": "^3",
    "turndown": "^5",
    "typescript": "^3"
  }
}
